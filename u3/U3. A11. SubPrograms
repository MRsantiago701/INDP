Subproceso areacubo
    Escribir "Ingrese el radio del cubo:"
    Leer radio
    area <- 3.1416 * radio * radio
    Escribir "El área del cubo es: ", area
FinSubproceso

Subproceso areaCilidro
    Escribir "Ingrese el radio del cilindro:"
    Leer ra
    area <- ra * altura
    Escribir "El área del cuadrado es: ", area
FinSubproceso
